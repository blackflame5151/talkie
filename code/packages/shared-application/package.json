{
  "name": "@talkie/shared-application",
  "version": "0.0.0",
  "author": {
    "email": "code+github@joelpurra.com",
    "name": "Joel Purra",
    "url": "https://joelpurra.com/"
  },
  "bundledDependencies": [
    "react",
    "react-redux",
    "styletron-react",
    "type-fest"
  ],
  "dependencies": {
    "@talkie/shared-application-helpers": "*",
    "@talkie/shared-locales": "*",
    "@talkie/split-environment": "*",
    "@talkie/split-environment-interfaces": "*"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "react-markdown": "^6.0.3",
    "redux": "^4.1.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styletron-standard": "^3.0.5",
    "webextension-polyfill": "^0.8.0"
  },
  "exports": {
    "./*": {
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    }
  },
  "homepage": "https://joelpurra.com/projects/talkie/",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "build": "npm run --silent build:typescript",
    "build:typescript": "tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json",
    "clean": "rimraf ./dist",
    "rebuild": "npm run --silent clean && npm run --silent build",
    "watch": "npm run --silent build -- --watch"
  },
  "typesVersions": {
    "^4": {
      "*": [
        "src/*"
      ]
    }
  }
}
