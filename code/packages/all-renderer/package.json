{
  "name": "@talkie/all-renderer",
  "version": "0.0.0",
  "author": {
    "email": "code+github@joelpurra.com",
    "name": "Joel Purra",
    "url": "https://joelpurra.com/"
  },
  "dependencies": {
    "@talkie/demo-renderer": "*",
    "@talkie/options-renderer": "*",
    "@talkie/popup-renderer": "*",
    "@talkie/renderer": "*",
    "@talkie/shared-locales": "*",
    "@talkie/split-environment-interfaces": "*",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "@types/mkdirp": "^1.0.2",
    "redux": "^4.1.1"
  },
  "exports": {
    "./*": {
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    }
  },
  "homepage": "https://joelpurra.com/projects/talkie/",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "build": "npm run --silent build:typescript && npm run --silent build:rollup",
    "build:rollup": "rollup --config rollup.config.js",
    "build:typescript": "tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json",
    "clean": "rimraf ./dist",
    "rebuild": "npm run --silent clean && npm run --silent build",
    "watch": "npm run --silent build:rollup -- --watch --watch.buildDelay 5000"
  },
  "typesVersions": {
    "^4": {
      "*": [
        "src/*"
      ]
    }
  }
}
