{
  "name": "@talkie/popup-renderer",
  "version": "0.0.0",
  "author": {
    "email": "code+github@joelpurra.com",
    "name": "Joel Purra",
    "url": "https://joelpurra.com/"
  },
  "dependencies": {
    "@talkie/browser-localeredirect": "*",
    "@talkie/popup-application": "*",
    "@talkie/renderer": "*",
    "redux": "^4.1.1"
  },
  "devDependencies": {
    "react": "^17.0.2"
  },
  "exports": {
    "./*": {
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    }
  },
  "homepage": "https://joelpurra.com/projects/talkie/",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "build": "npm run --silent build:typescript",
    "build:typescript": "tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json",
    "clean": "rimraf ./dist",
    "rebuild": "npm run --silent clean && npm run --silent build",
    "watch": "npm run --silent build -- --watch"
  },
  "typesVersions": {
    "^4": {
      "*": [
        "src/*"
      ]
    }
  }
}
