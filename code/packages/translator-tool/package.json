{
  "name": "@talkie/translator-tool",
  "version": "0.0.0",
  "author": {
    "email": "code+github@joelpurra.com",
    "name": "Joel Purra",
    "url": "https://joelpurra.com/"
  },
  "bundledDependencies": [
    "type-fest"
  ],
  "dependencies": {
    "@google-cloud/translate": "^6.2.7",
    "bluebird": "^3.7.2",
    "clone": "^2.1.2",
    "configvention": "^2.0.0",
    "ent": "^2.2.0",
    "jsonfile": "^6.1.0",
    "striptags": "^3.2.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/clone": "^2.1.1",
    "@types/ent": "^2.2.5",
    "@types/jsonfile": "^6.0.1"
  },
  "exports": {
    "./*": {
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    }
  },
  "homepage": "https://joelpurra.com/projects/talkie/",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "build": "npm run --silent build:typescript && npm run --silent build:rollup",
    "build:rollup": "rollup --config rollup.config.js",
    "build:typescript": "tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json",
    "clean": "rimraf ./dist",
    "rebuild": "npm run --silent clean && npm run --silent build",
    "watch": "npm run --silent build -- --watch"
  },
  "typesVersions": {
    "^4": {
      "*": [
        "src/*"
      ]
    }
  }
}
